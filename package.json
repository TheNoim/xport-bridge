{
  "name": "xport-bridge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && nuxt-ts generate --spa",
    "build:nuxt": "nuxt-ts generate --spa",
    "build:mikro-orm-cache": "nest build && mikro-orm cache:generate",
    "nuxt:dev": "cross-env NODE_ENV=development nuxt-ts generate --spa --devtools",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.0.0",
    "@mikro-orm/entity-generator": "^5.0.0",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/sqlite": "^5.0.0",
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/serve-static": "^2.1.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.9.5",
    "bootstrap-vue": "^2.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.0",
    "cross-spawn-promise": "^0.10.2",
    "json-beautify": "^1.1.1",
    "mikro-orm": "^5.0.0",
    "nomnom": "^1.8.1",
    "nuxt": "^2.12.0",
    "p-queue": "^6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.5.4",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.0.0",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.2.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.2",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/database/database-config.ts",
      "./dist/database/database-config.js"
    ]
  }
}
